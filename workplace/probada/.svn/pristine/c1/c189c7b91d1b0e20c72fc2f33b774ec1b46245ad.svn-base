		return url;
	}
	
	@RequestMapping("/my-page")
	public String my_page() {
		String url = "/web-app/my-page/my-page";
		return url;
	}
	
	@RequestMapping("/pricing")
	public String pricing() {
		String url = "/web-app/common/pricing";
		return url;
	}
	
	
}
