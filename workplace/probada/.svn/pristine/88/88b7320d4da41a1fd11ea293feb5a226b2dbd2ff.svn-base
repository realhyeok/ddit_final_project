/* project-detail */
.project-member {
  text-align: center;
}
.project-title {
  font-size: 1.3em;
  font-weight: bold;
}
.project-member img {
  width: 50px;
  height: 50px;
}
.project-detail_body {
  padding-bottom: 20px;
}
.project-bg-btn {
  margin: 0 auto;
}
.header-title {
  display: flex;
  justify-content: space-between;
}

/* project-task */
.task-member {
  text-align: center;
}
.task-bold {
  font-weight: bold;
}
.task-sm {
  font-size: 0.8em;
}
.task-member img {
  width: 50px;
  height: 50px;
}
.task-detail-body {
  padding-bottom: 20px;
}
.task-bg-btn {
  margin: 0 auto;
}

/project-list/
.project-list.card {
  margin-bottom: 10px;
}
.project-list-a {
  color: black;

}
.project-list-sort {
  margin-bottom: 5px;
}
.project-list-btn {
  width: 50px;
  height: 50px;
  float: right;
}
.card-body.project-list {
  padding-top: 0px;
  padding-bottom: 0px;
}