function on() {
	$("#popoverlay").fadeIn(300);
	var nav = document.querySelector("body");
	nav.classList.add("nav-sm");
	nav.classList.remove("nav-md");
}

function off() {
	$("#popoverlay").fadeOut(300);
	var nav = document.querySelector("body");
	nav.classList.add("nav-md");
	nav.classList.remove("nav-sm");
}
