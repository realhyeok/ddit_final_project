<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="IssueReply-Mapper">

	<select id="selectIssueReplyListByIssueNo" resultType="issueReply" parameterType="String">
		SELECT
    		ISSUE_RES_NO
   			,ISSUE_NO
   			,USER_ID
   			,PROJ_NO
   			,CONTENT
   			,REGDATE
   			,UPDATEDATE
		FROM
    		ISSUE_RES
		WHERE
   			ISSUE_NO = #{issueNo}
	</select>

</mapper>